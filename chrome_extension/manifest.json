{
  "name": "Elasticvue",
  "version": "0.10.2",
  "description": "Elasticsearch frontend",
  "manifest_version": 2,
  "icons": {
    "16": "./assets/img/logo/logo_16_blue.png",
    "48": "./assets/img/logo/logo_48_blue.png",
    "128": "./assets/img/logo/logo_128_blue.png"
  },
  "background": {
    "scripts": [
      "background.js"
