    "ace-builds": "^1.23.1",
    "idb": "^7.1.1",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0",
    "pretty-bytes": "^6.1.0",
    "quasar": "^2.12.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-resizable": "^2.1.7",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.1",
