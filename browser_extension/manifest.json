{
  "name": "Elasticvue",
  "version": "0.13.0",
  "description": "Elasticsearch frontend",
  "manifest_version": 2,
  "icons": {
    "16": "./assets/img/logo/blue_16.png",
    "32": "./assets/img/logo/blue_32.png",
    "48": "./assets/img/logo/blue_48.png",
    "128": "./assets/img/logo/blue_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
